{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd farcaster-v2-frame-template && yarn install && yarn build",
  "installCommand": "cd farcaster-v2-frame-template && yarn install",
  "outputDirectory": "farcaster-v2-frame-template/.next",
  "redirects": [
    {
      "source": "/",
      "destination": "/frames",
      "permanent": false
    }
  ],
  "env": {
    "NEXTAUTH_SECRET": "USE_A_STRONG_SECRET_HERE",
    "NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID": "3927fffa4d437bd3d91e58cb4eae4a50",
    "NEYNAR_API_KEY": "YOUR_NEYNAR_API_KEY_HERE",
    "NEXT_PUBLIC_URL": "${VERCEL_URL}"
  }
}